{
  "name": "flutter-ui",
  "version": "3.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:briefguo/flutter-ui.git",
  "author": "郭永杰 <gyj1206@icloud.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "start:docs": "SHOW_SCREEN_DEBUG=true parcel docs/index.html",
    "test": "jest",
    "start": "npm run start:docs",
    "watch": "tsc -d -w",
    "build": "tsc -d && cp src/index.css dist/index.css"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "classnames": "^2.2.6",
    "color-js": "^1.0.5",
    "immutable": "^4.0.0-rc.12",
    "md5": "^2.3.0",
    "react-responsive": "^8.2.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/md5": "^2.3.0",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "react-is": "^16.13.1",
    "react-responsive": "^8.2.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.0.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  }
}
